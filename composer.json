{
    "name": "phlexible/sitemap-bundle",
    "description": "phlexible sitemap bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["sitemap", "xml"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\SitemapBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/tree-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "symfony/config": "~2.8",
        "symfony/dependency-injection": "~2.8",
        "symfony/http-kernel": "~2.8"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/repo/private/"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
