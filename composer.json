{
    "name": "phlexible/sitemap-bundle",
    "description": "phlexible sitemap bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["sitemap", "xml"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\SitemapBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "thepixeldeveloper/sitemap": "^4.4.2",
        "phlexible/siteroot-bundle": "~1.3",
        "phlexible/tree-bundle": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "symfony/config": "^2.8",
        "symfony/console": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/http-kernel": "^2.8",
        "doctrine/collections": "^1.3",
        "mikey179/vfsStream": "^1.6",
        "phlexible/country-context-bundle": "~1.2",
        "matthiasnoback/symfony-config-test": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
